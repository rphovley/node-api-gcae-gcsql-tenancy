  },
  "author": "FUNDZERO",
  "license": "UNLICENSED",
  "scripts": {
    "start": "bash scripts/start-containers.sh",
    "stop": "docker-compose -p f0-datalake -f docker-compose.dev.yml down",
    "kill": "docker-compose -p f0-datalake -f docker-compose.dev.yml kill",
    "restart": "docker-compose -p f0-datalake -f docker-compose.dev.yml restart",
    "logs": "docker-compose -p f0-datalake -f docker-compose.dev.yml logs -f",
    "connect": "docker exec -it f0-datalake_api bash",
    "connectdb": "docker exec -it f0-datalake_api_db psql -U f0-datalake",
    "build-image": "bash scripts/build-docker-image.sh",
    "deploy": "bash scripts/deploy-docker-image.sh",
    "lint": "gulp lint",
    "migrations": "echo I need migrations, man",
    "test": "bash scripts/run-tests.sh",
    "test:setup": "docker-compose -p f0-datalake_test -f docker-compose.test.yml up --force-recreate -d; docker-compose -p f0-datalake_test -f docker-compose.test.yml logs -f",
    "test:stop": "docker-compose -p f0-datalake_test -f docker-compose.test.yml down",
    "test:run": "./node_modules/mocha/bin/mocha \"test/**/*.test.js\"",
    "watch": "tsc -w -p ./",
    "compile": "tsc -p ./"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "bookshelf": "^1.0.1",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "knex": "^0.19.5",
    "morgan": "^1.9.1",
    "objection": "^1.6.11",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.12.1"
  },
  "devDependencies": {
    "@types/bookshelf": "^0.13.3",
    "@types/express": "^4.17.1",    
    "@types/knex": "^0.16.1",
    "@types/passport": "^1.0.1",
    "@types/passport-jwt": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.5.0",
    "eslint": "^6.6.0",
    "gulp": "^4.0.2",
    "gulp-eslint": "^6.0.0",
    "gulp-nodemon": "^2.4.2",
    "gulp-typescript": "^5.0.1",
    "mocha": "^6.2.1",
    "typescript": "^3.6.4"
  }
